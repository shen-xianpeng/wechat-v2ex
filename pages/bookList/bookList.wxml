<!--latest.wxml-->

  <view class="container">

    <view class="item" wx:for="{{datalist}}" wx:key="id">
      <image     mode="aspectFit" src="{{item.image}}"></image>
      <view class="text">
        <text class="title">{{item.title}}</text>
        <text class="description">
          {{item.publisher}}</text>
      </view>
    </view>


  </view>


