page, .page, .page__bd {
  height: 100%;
}

.page__bd {
  padding-bottom: 0;
}

.weui-tab__content {
}

.nav-menu {
  background: white;
}

.coin-wrapper {
  width: 100%;
  height: 300rpx;
}

.coin-num {
  display: block;
  width: 200rpx;
  height: 200rpx;
  background: #f9b73b;
  margin: 0 auto;
  margin-top: 50rpx;
  border-radius: 100rpx;
  text-align: center;
  color: white;
  line-height: 200rpx;
  font-size: 70rpx;
}

.money-wrapper {
  width: 100%;
  height: 300rpx;
  position: relative;
}

.money-num {
  display: block;
  width: 200rpx;
  height: 200rpx;
  background: lightgreen;
  margin: 0 auto;
  margin-top: 50rpx;
  border-radius: 100rpx;
  text-align: center;
  color: white;
}

.widthdraw-btn {
  position: absolute;
  right: 80rpx;
  top: 110rpx;
  padding: 10rpx 20rpx 10rpx 20rpx;
  background: gray;
  border-radius: 8rpx;
  font-size: 30rpx;
  color: white;
}

.coin-num-amount {
  line-height:180rpx;
  font-size: 66rpx;
  height:130rpx;

}

.coin-num-label {
  line-height:60rpx;
  font-size:36rpx;
  height:70rpx;

}

.money-num-amount {
  line-height:180rpx;
  font-size: 66rpx;
  height:130rpx;

}

.money-num-label {
  line-height:60rpx;
